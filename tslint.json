{
  "defaultSeverity": "warning",
  "extend": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "interface-name": false,
    "class-name": true,
    "no-unsafe-any": true,
    "quotemark": [true, "single"],
    "indent": [false],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-consecutive-blank-lines": false,
    "no-debugger": false,
    "member-access": false,
    "prettier": true,
    "semicolon": [
      true,
      "always",
      "ignore-interfaces"
    ]
  }
}
